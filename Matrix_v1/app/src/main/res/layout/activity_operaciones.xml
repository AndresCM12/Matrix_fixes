<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".OperacionesActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:id="@+id/constraint"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/guideline5"
        app:layout_constraintLeft_toLeftOf="@id/guideline"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        android:background="@drawable/repeat_grid_2"
        android:elevation="6dp"
        android:paddingBottom="30dp"
        android:paddingTop="10dp">

        <GridLayout
            android:id="@+id/gridLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alignmentMode="alignBounds"
            app:layout_constraintBottom_toBottomOf="@id/guidel2"
            app:layout_constraintLeft_toLeftOf="@id/guidel"
            app:layout_constraintRight_toRightOf="@id/guidel"
            app:layout_constraintTop_toTopOf="@id/guidel2">

            <EditText
                android:id="@+id/txt1"
                android:textSize="14sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="0"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txt2"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="1"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txt3"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="2"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txt6"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="0"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txt7"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="1"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txt8"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="2"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txt11"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="0"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txt12"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="1"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txt13"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="2"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txtA"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="3"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txtB"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="3"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

            <EditText
                android:id="@+id/txtC"
                android:textSize="14sp"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="3"
                android:enabled="false"
                android:inputType="numberDecimal|numberSigned"
                android:textAlignment="center"
                android:visibility="gone" />

        </GridLayout>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent=".5" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidel2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent=".5" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/btnVolver"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/volver"
        android:fontFamily="sans-serif-condensed-light"
        android:text="Regresar"
        android:textColor="@android:color/background_light"
        android:textSize="12sp"
        app:layout_constraintLeft_toLeftOf="@id/guideline"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintTop_toTopOf="@id/guideline3"
        app:layout_constraintBottom_toBottomOf="@id/guideline4"/>

    <Button
        android:id="@+id/btnPasos"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:background="@drawable/repeat_grid_1"
        android:fontFamily="sans-serif-condensed-light"
        android:text="@string/pasos"
        android:textColor="@android:color/background_light"
        android:textSize="11sp"
         app:layout_constraintLeft_toLeftOf="@id/guideline"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintBottom_toTopOf="@id/btnVolver"
        android:layout_marginBottom="2dp"/>

    <TextView
        android:id="@+id/txtPy"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_bold_italic"
        android:gravity="center"
        android:inputType="textMultiLine"
        android:textColor="@color/gris"
        android:textSize="14sp"
        android:visibility="invisible"
         app:layout_constraintLeft_toLeftOf="@id/guideline"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintTop_toBottomOf="@id/constraint"/>

    <TextView
        android:id="@+id/txtResultado"
        android:layout_width="282dp"
        android:layout_height="59dp"
        android:fontFamily="sans-serif-light"
        android:gravity="center_horizontal"
        android:text="ejemplo"
        android:textColor="@color/gris"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPy"
        app:layout_constraintVertical_bias="0.283" />

    <TextView
        android:id="@+id/txtResultado2"
        android:layout_width="96dp"
        android:layout_height="39dp"
        android:fontFamily="sans-serif-light"
        android:gravity="center_horizontal"
        android:text="@string/porque"
        android:textColor="@color/ic_launcher_background"
        android:textSize="18sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtResultado"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtResultado3"
        android:layout_width="289dp"
        android:layout_height="53dp"
        android:fontFamily="sans-serif-light"
        android:gravity="center_horizontal"
        android:inputType="textMultiLine"
        android:text="ejemplo"
        android:textColor="@color/gris"
        android:textSize="18sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.468"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtResultado2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtVwEcu"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:background="@color/colorPrimaryDark"
        android:fontFamily="sans-serif-light"
        android:onClick="onClickTxtVw"
        android:padding="7dp"
        android:text="Ver cómo se calculan"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/txtResultado"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPy"
        app:layout_constraintVertical_bias="0.085" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".15" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".85" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".85"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".92"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".15"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".08"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".5"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline77"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".585"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline777"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".665"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7777"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".745"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".5"/>

    <TextView
        android:id="@+id/txtUno"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-light"
        android:gravity="center_horizontal"
        android:text="Ultima Iteración"
        android:textSize="25dp"
        app:layout_constraintTop_toTopOf="@id/guideline6"
        app:layout_constraintLeft_toLeftOf="@id/guideline"
        app:layout_constraintRight_toRightOf="@id/guideline2"/>


</androidx.constraintlayout.widget.ConstraintLayout>



